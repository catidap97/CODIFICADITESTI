	<text xml:lang="it" xml:id="Pag125-126-127" subtype="manuscript">
		<body>
			<div type="pagina" n="125">
				<pb n="125" facs="#P125Intera" />
				<!-- Inizio pagina -->
				<ab type="pericope" n="1" xml:id="Pag125Pericope1">
					<lb xml:id="P125D" n="0" facs="#P125Data" />
					30 dic.
					<lb xml:id="P125R1" n="1" facs="#P125Riga1" />
					Ieri pomeriggio si ebbe l'impressione 
					
					<lb xml:id="P125R2" n="2" facs="#P125Riga2" />
					dell'enorme confusione creata da 
                    
					<lb xml:id="P125R3" n="3" facs="#P125Riga3" />
                    tedeschi: circa un terzo delle 

					<lb xml:id="P125R4" n="4" facs="#P125Riga4" />
                    provviste sono state disperse o rubate, 

					<lb xml:id="P125R5" n="5" facs="#P125Riga5" />
                    o perdute, non si trovano interi

					<lb xml:id="P125R6" n="6" facs="#P125Riga6" />
                    magazzini e d'altra parte 

					<lb xml:id="P125R7" n="7" facs="#P125Riga7" />
                    molti atti di brigantaggio di 

					<lb xml:id="P125R8" n="8" facs="#P125Riga8" />
                    sbandati spaventano la popola-

					<lb xml:id="P125R9" n="9" facs="#P125Riga9" />
                    zione contro di noi. 
                </ab>
				<ab type="pericope" n="2" xml:id="Pag125Pericope2">
					Hanno ragione 
					<lb xml:id="P125R10" n="10" facs="#P125Riga10" />
                    i tedeschi, per disorganizzare 

					<lb xml:id="P125R11" n="11" facs="#P125Riga11" />
                    le bande non è necessario fare 

					<lb xml:id="P125R12" n="12" facs="#P125Riga12" />
                    battute difficili, basta risalire 

					<lb xml:id="P125R13" n="13" facs="#P125Riga13" />
                    le vie di camminazione grandi. 
				</ab>
				<ab type="pericope" n="3" xml:id="Pag125Pericope3">

					<lb xml:id="P125R14" n="14" facs="#P125Riga14" />
                    Mi vengono in mente 4 brutti versi 

					<lb xml:id="P125R15" n="15" facs="#P125Riga15" />
                    che nel carnvevale del una 

					<lb xml:id="P125R16" n="16" facs="#P125Riga16" />
                    maschera disse a Carlo Alberto 
			    </ab>
				<ab type="pericope" n="4" xml:id="Pag125Pericope4">

					<lb xml:id="P125R17" n="17" facs="#P125Riga17" />
                    <l>L'italia è guerra <gap reason="illegible" unit="character" quantity="5"/></l>

					<lb xml:id="P125R18" n="18" facs="#P125Riga18" />
                    <l>dal tempo</l>

					<lb xml:id="P125R19" n="19" facs="#P125Riga19" />
                    <l>e i paperi col becco</l>

					<lb xml:id="P125R20" n="20" facs="#P125Riga20" />
                    <l>tentano <gap reason="illegible" unit="character" quantity="6"/> invani.</l>
				</ab>
				<ab type="pericope" n="5" xml:id="Pag125Pericope5">

					<lb xml:id="P125R21" n="21" facs="#P125Riga21" />
                    Ieri conobbi anche il famoso Walter, 
				</ab>
				
			</div>
					
			<div type="pagina" n="126">
				<pb n="126" facs="#P126Intera" />
				<!-- Inizio pagina -->
				<ab type="pericope" n="1" xml:id="Pag126Pericope1">
					
					<lb xml:id="P126R1" n="1" facs="#P126Riga1" />
					insieme, G. e io non vedremmo mai un viso fa-
					
					<lb xml:id="P126R2" n="2" facs="#P126Riga2" />
					migliare. Per questo G mi prega sempre di non
				</ab>
                <ab type="pericope" n="2" xml:id="Pag126Pericope2">

					<lb xml:id="P126R3" n="3" facs="#P126Riga3" />
					lasciare B e io <del rend="overstrike">sono</del> titubo prima di lasciarlo,

					<lb xml:id="P126R4" n="4" facs="#P126Riga4" />
					fu un momento difficile, malato o ferito, come 

					<lb xml:id="P126R5" n="5" facs="#P126Riga5" />
					farà tutto solo? Come farei se pensassi lui nelle 
				</ab>
		        <ab type="pericope" n="3" xml:id="Pag126Pericope3">

					<lb xml:id="P126R6" n="6" facs="#P126Riga6" />
					medesime condizioni? Li aggiunga che per il sol-
                </ab>
				<ab type="pericope" n="4" xml:id="Pag126Pericope4">

					<lb xml:id="P126R7" n="7" facs="#P126Riga7" />
					dato la prigionia può <del rend="overstrike">po</del> essere anche un rigugio;

					<lb xml:id="P126R8" n="8" facs="#P126Riga8" />
					per noi è la morte, e che morte?
                </ab>
				<ab type="pericope" n="5" xml:id="Pag126Pericope5">
                    La morte

					<lb xml:id="P126R9" n="9" facs="#P126Riga9" />
					di quel partigiano a cui i Ted strapparono la 

					<lb xml:id="P126R10" n="10" facs="#P126Riga10" />
					moglie pochi giorni fa prima di <gap reason="illegible" unit="character" quantity="8"/>. 
				</ab>
				<ab type="pericope" n="6" xml:id="Pag126Pericope6">
					Linea 
		
					<lb xml:id="P126R11" n="11" facs="#P126Riga11" />
					del fronte sono tutte le strade dove,<del rend="overstrike">da</del> ad una 

					<lb xml:id="P126R12" n="12" facs="#P126Riga12" />
					svolta si può trovare un gruppo di fascisti, ?

					<lb xml:id="P126R13" n="13" facs="#P126Riga13" />
					i prati, i borghi, i sentierini dove cerchiamo 

					<lb xml:id="P126R14" n="14" facs="#P126Riga14" />
					di andare nascosti, <gap reason="illegible" unit="character" quantity="6"/> dei partigiani;

					<lb xml:id="P126R15" n="15" facs="#P126Riga15" />
					vedono dei fascisti, <del rend="overstrike">la gaz</del> i giornali tutti i giorni 

					<lb xml:id="P126R16" n="16" facs="#P126Riga16" />
					<gap reason="illegible" unit="character" quantity="9"/> nuove, strade e la guerra procede

					<lb xml:id="P126R17" n="17" facs="#P126Riga17" />
					nel tempo lentissima e pesante come una mo- 

					<lb xml:id="P126R18" n="18" facs="#P126Riga18" />
					struosa enorme lumaca, che lasci dietro di 

					<lb xml:id="P126R19" n="19" facs="#P126Riga19" />
					se una <del rend="overstrike">? </del> 
                </ab>
				<ab type="pericope" n="7" xml:id="Pag126Pericope7">
                Larga striscia di sangue

					<lb xml:id="P126R20" n="20" facs="#P126Riga20" />
					incapaci di <gap reason="illegible" unit="character" quantity="6"/>, come hanno rifiutato quel 

					<lb xml:id="P126R21" n="21" facs="#P126Riga21" />
					magnifico guerriero di Zama <del rend="overstrike">diremo</del> quali-

					<lb xml:id="P126R22" n="22" facs="#P126Riga22" />
					ficandolo un filibustiere. 
				 </ab>
				
			</div>


			<div type="pagina" n="127">
				<pb n="127" facs="#P127Intera" />
				<!-- Inizio pagina -->
				<ab type="pericope" n="1" xml:id="Pag127Pericope1">
			
					<lb xml:id="P127R1" n="1" facs="#P127Riga1" />
					Giovano <add place="above">il</add> <emph rend="underline">pane e burro</emph> <add place="above">della banda</add> <emph rend="circled">tutto il giorno </emph>
			
					<lb xml:id="P127R2" n="2" facs="#P127Riga2" />
					che se T.P. è <del rend="overstrike">per</del>strategicamente 
                    
					<lb xml:id="P127R3" n="3" facs="#P127Riga3" />
                    infelice perchè ha una sola <gap reason="illegible" unit="character" quantity="5"/> 

					<lb xml:id="P127R4" n="4" facs="#P127Riga4" />
                    mobile, tengano 50 uomini sui <gap reason="illegible" unit="character" quantity="4"/> 

					<lb xml:id="P127R5" n="5" facs="#P127Riga5" />
                    <gap reason="illegible" unit="character" quantity="5"/> e mandino altrove gli altri 

					<lb xml:id="P127R6" n="6" facs="#P127Riga6" />
                    350, che i non hanno ragione di 

					<lb xml:id="P127R7" n="7" facs="#P127Riga7" />
                    chiamare secondo la teoria mar-

					<lb xml:id="P127R8" n="8" facs="#P127Riga8" />
                    xista il nostro un partito di classe,

					<lb xml:id="P127R9" n="9" facs="#P127Riga9" />
                    di classe borghese.  
                </ab>
				<ab type="pericope" n="2" xml:id="Pag127Pericope2">
				    si <gap reason="illegible" unit="character" quantity="12"/> 

					<lb xml:id="P127R10" n="10" facs="#P127Riga10" />
                    <gap reason="illegible" unit="character" quantity="7"/>i partigiani, non gli sfollati
                </ab>
				<ab type="pericope" n="3" xml:id="Pag127Pericope3">
					<lb xml:id="P127R11" n="11" facs="#P127Riga11" />
                     Mio nonno entrato nel T. di G per 

					<lb xml:id="P127R12" n="12" facs="#P127Riga12" />
                    essere adoperato, date le circostanze
		
					<lb xml:id="P127R13" n="13" facs="#P127Riga13" />
                    possiamo dimetterci è la nostra  
				
					<lb xml:id="P127R14" n="14" facs="#P127Riga14" />
                    opera per la libertà non sarà meno-

					<lb xml:id="P127R15" n="15" facs="#P127Riga15" />
                    mata, perchè mi <gap reason="illegible" unit="character" quantity="4"/>al part. non facciamo nulla 
                
				 </ab>
				<ab type="pericope" n="4" xml:id="Pag127Pericope4">
